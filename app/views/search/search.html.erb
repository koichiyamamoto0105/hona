<div class = "container">
    <div class="row">

        <div class='col-lg-5 col-lg-offset-1'>

          <% if @model == 'map' %>
           <h3><%= t('.result_spot')%> "<%= @content %>"</h3>
            <%= render 'maps/index', maps: @records %>
           <h5><%= t('.create_spot')%> =><%=link_to t('.link_to_map'), maps_path, class: "cp_link" %></h5>
          <% elsif @model == 'hashtag' %>
          	<h3><%= t('.result_hashtag')%>"<%= @content %>"</h3>
            <%= render 'hashtags/index', hashtags: @records %>
          <% end %>

        </div>

        <div class ="col-lg-6">
        </div>
     </div>
</div>